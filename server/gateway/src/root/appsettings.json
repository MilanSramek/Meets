{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "connect": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/connect/{**remainder}"
        },
        "Methods": [
          "POST"
        ]
      },
      "account": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/account/register"
        },
        "Methods": [
          "POST"
        ]
      },
      "wellKnown": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/.well-known/{**remainder}"
        },
        "Methods": [
          "GET"
        ]
      }
    },
    "Clusters": {
      "identity": {
        "Destinations": {
          "destination1": {
            "Address": "http://identity"
          }
        }
      }
    }
  }
}